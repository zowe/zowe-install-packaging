{
  "compilerOptions": {
    "lib": [ "ES2020" ],
    "module": "ES2020",
    "target": "ES2020",
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "pretty": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": false,
    "allowUnreachableCode": false,
    "allowJs": false,
    "listEmittedFiles": true,
    "strict": true,
    
  },
  "include": [
    "../bin/libs/*",
    "../bin/commands/**/*"
  ],
  "exclude": [
    "../bin/libs/json.ts"
  ]
}
